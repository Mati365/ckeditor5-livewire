{
  "name": "mati365/ckeditor5-livewire",
  "version": "1.0.1",
  "description": "CKEditor 5 integration for Laravel Livewire",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Mati365\\CKEditor5Livewire\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mati365\\CKEditor5Livewire\\ServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Mateusz Baginski",
      "email": "cziken58@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "illuminate/support": "^11.0 || ^12.0",
    "livewire/livewire": "^3.0",
    "respect/validation": "^2.4"
  },
  "require-dev": {
    "psalm/plugin-laravel": "^3.0.4",
    "vimeo/psalm": "^6.13",
    "friendsofphp/php-cs-fixer": "^3.88",
    "phpunit/phpunit": "^11.0",
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^9.0"
  },
  "scripts": {
    "test": "phpunit",
    "test:coverage": "env XDEBUG_MODE=coverage php -d pcov.enabled=1 -d pcov.directory=. ./vendor/bin/phpunit --coverage-html coverage --coverage-text"
  },
  "autoload-dev": {
    "psr-4": {
      "Mati365\\CKEditor5Livewire\\Tests\\": "tests/"
    }
  }
}
