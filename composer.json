  "description": "CKEditor 5 integration for Laravel Livewire",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Mati365\\CKEditor5Livewire\\": "src/"
    }
  },
  "extra": {
    "laravel": {
