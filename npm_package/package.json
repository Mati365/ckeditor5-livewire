{
  "name": "ckeditor5-livewire",
  "version": "1.3.0",
  "description": "CKEditor 5 integration for Laravel Livewire",
  "author": "Mateusz Bagi≈Ñski <cziken58@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mati365/ckeditor5-livewire"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./bundler/vite-ckeditor5-externalize": {
      "types": "./dist/bundler/vite-ckeditor5-externalize.d.ts",
      "import": "./dist/bundler/vite-ckeditor5-externalize.mjs",
      "require": "./dist/bundler/vite-ckeditor5-externalize.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "bundler",
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "prepare": "pnpm run build",
    "build": "pnpm run clean && vite build && node ./scripts/check-dist-imports.mjs",
    "watch": "vite build --watch",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit --pretty",
    "test": "vitest --coverage",
    "test:watch": "vitest --watch --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "ckeditor5": "^45.2.2",
    "ckeditor5-premium-features": "^45.2.1",
    "happy-dom": "^20.0.1",
    "typescript": "^5.5.4",
    "vite": "^7.1.11",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  }
}
